{
  "name": "shared-styles-shared-styles",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/styles/shared-styles/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/styles/shared-styles",
        "main": "libs/shared/styles/shared-styles/src/index.ts",
        "tsConfig": "libs/shared/styles/shared-styles/tsconfig.lib.json",
        "assets": ["libs/shared/styles/shared-styles/*.md", {
          "input": "libs/shared/styles/shared-styles/src/lib", 
          "glob": "**/*.(scss)",
          "output": "./src"
        }]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs shared-styles-shared-styles {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/styles/shared-styles/**/*.ts"]
      }
    }
  },
  "tags": []
}
